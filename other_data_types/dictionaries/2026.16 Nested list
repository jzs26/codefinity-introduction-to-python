#get a value from a nested list

nested_list = [[10, 20, 30], [44, 55, 66], [77, 87, 99]]
value = nested_list[1][1]
#print(value)

#flatten nested list
list_of_lists = [[1, 2], [3, 4], [5, 6, 7]]
#expected output = [1, 2, 3, 4, 5, 6, 7]
def flatten_list_comprehension(list_of_lists):
    return[item for sublist in list_of_lists for item in sublist]
flatten_list_result = flatten_list_comprehension(list_of_lists)
print(flatten_list_result)
