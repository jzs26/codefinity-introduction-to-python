list1 = [10, 20, 25, 30, 35]
list2 = [40, 45, 60, 75, 90]
#result list: [25, 35, 40, 60, 90]

odd_numbers = []

for num in list1:
    if num % 2 != 0:
        odd_numbers.append(num)
even_numbers = []
for num in list2:
    if num %2 == 0:
        even_numbers.append(num)
result_list = odd_numbers + even_numbers
print("The result list is:", result_list)
